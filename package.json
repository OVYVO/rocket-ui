{
  "name": "rockets-ui",
  "version": "1.0.2",
  "description": "适用于vue2.0的组件库",
  "license": "ISC",
  "main": "lib/components.es.js",
  "keywords": [
    "rockets-ui"
  ],
  "files": [
    "lib",
    "src",
    "packages"
  ],
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "core-js": "^3.8.3",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/eslint-parser": "^7.19.1",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@rollup/plugin-alias": "^3.1.9",
    "@vuepress/plugin-back-to-top": "^1.9.7",
    "autoprefixer": "9.0.0",
    "babel-loader": "^8.2.2",
    "commitizen": "^4.2.5",
    "commitlint-config-gitmoji": "^2.2.5",
    "cross-env": "^7.0.3",
    "cz-emoji": "^1.3.2-canary.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "file-save": "^0.2.0",
    "husky": "^8.0.1",
    "json-templater": "^1.2.0",
    "less-loader": "7",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rollup": "^2.38.4",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "uppercamelcase": "^3.0.0",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^15.9.6",
    "vue-server-renderer": "2.6.12",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.9.7",
    "vuepress-plugin-demo-container": "^0.2.0"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "build": "cross-env NODE_ENV=production rollup -c",
    "build:dev": "cross-env NODE_ENV=development rollup -c",
    "cz": "git add . && git-cz",
    "prepare": "husky install",
    "lint:prettier": "npx prettier -w ."
  },
  "author": "vue12306@163.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/OVYVO/rocket-ui.git"
  },
  "bugs": {
    "url": "https://github.com/OVYVO/rocket-ui/issues"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ],
  "config": {
    "cz-emoji": {
      "types": [
        {
          "emoji": "✨",
          "code": ":sparkles:",
          "description": "一个新功能",
          "name": "feat"
        },
        {
          "emoji": "🐛",
          "code": ":bug:",
          "description": "修复一个Bug",
          "name": "fix"
        },
        {
          "emoji": "💄",
          "code": ":lipstick:",
          "description": "代码风格，格式修复",
          "name": "style"
        },
        {
          "emoji": "⚡️",
          "code": ":zap:",
          "description": "代码优化,改善性能",
          "name": "perf"
        },
        {
          "emoji": "📝",
          "code": ":memo:",
          "description": "文档变更",
          "name": "docs"
        }
      ],
      "skipQuestions": [
        "scope",
        "body",
        "breaking",
        "issues"
      ],
      "questions": {
        "type": "请选择提交类型(必填):",
        "subject": "请简要描述提交(必填):"
      },
      "subjectMaxLength": 100
    },
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && npx cz --hook || true"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
